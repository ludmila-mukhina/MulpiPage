<Page x:Class="MultiPageApplication.AutoPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MultiPageApplication"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AutoPage">

    <Grid>
        <StackPanel>
            <TextBlock>Страница авторизации</TextBlock>
            <GroupBox Header="Введите логин">
                <TextBox Name="TBAutoLogin" Width="250" HorizontalAlignment="Left"></TextBox>
            </GroupBox>
            <GroupBox Header="Введите пароль">
                <PasswordBox Name="PBAutoPassword" Width="250" HorizontalAlignment="Left"></PasswordBox>
            </GroupBox>
            <Button Name="BtnAuto" Click="BtnAuto_Click">Авторизация</Button>
            <Button Click="Button_Click">Вернуться на главную страницу</Button>
        </StackPanel>
    </Grid>
</Page>
